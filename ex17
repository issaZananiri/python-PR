import random
num = random.randint(1,9)
guess = 0
c = 0
while guess != num :
    guess = input("Enter a number between 1 to 10")

    

    guess = int(guess)
    c += 1

    if guess < num:
        print(" low")
    elif guess > num:
        print(" high")
    else:
        print("Right!")
        print( c, "tries")

