import random
def calc(num1,num2,choice):
    if (choice==1):
       print(num1-num2)
    elif(choice==2):
       print(num1+num2)
    elif(choice==3):
       print(num1/num2) 
    elif(choice==4):
       print(num1*num2)
print("enter first number:")       
nu1=input()
print("enter second number:") 
nu2=input()
seq=[1,2,3,4]
j=random.choice(seq)
calc(nu1,nu2,j)


         
