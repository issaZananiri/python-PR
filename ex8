def multiplication(num1,num2):
                result=num1*num2;
                print(result)

def division(num1,num2):
            if num2!=0:
                 result=num1/num2;
                 print(result)
            else:
                print("ERROR! Division by zero ")
             



a=input("enter dominator:")
b=input("enter numerator:")
choice=input("enter 1 for multiplication or 2 for division:")


if choice==1:
 multiplication(a,b)
 
elif choice==2:
      division(a,b)
      
       
        


           
